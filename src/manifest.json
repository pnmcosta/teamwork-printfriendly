{
  "name": "Teamwork Projects Print Friendly",
  "version": "0.1.0",
  "manifest_version": 2,
  "description": "Print Friendly Teamwork Projects Task Lists that removes all the unecessary UI elements and includes descriptions and time estimates",
  "homepage_url": "https://github.com/pnmcosta/teamwork-printfriendly/",
  "background": {
    "scripts": [
      "teamwork-background.js"
    ]
  },
  "icons": {
    "16": "icons/icon16.png",
    "24": "icons/icon24.png",
    "32": "icons/icon32.png",
    "128": "icons/icon128.png"
  },
  "browser_action": {
    "default_icon": {
      "16": "icons/icon16.png",
      "24": "icons/icon24.png",
      "32": "icons/icon32.png",
      "128": "icons/icon128.png"
    },
    "default_title": "Print Friendly Teamwork Projects!"
  },
  "permissions": [
    "https://*.teamwork.com/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://*.teamwork.com/*"
      ],
      "css": [
        "teamwork-print.css"
      ],
      "run_at": "document_start"
    }
  ]
}